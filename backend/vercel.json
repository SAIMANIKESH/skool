{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/chat", "dest": "index.js" },
    { "src": "/", "dest": "index.js" }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "env": {
    "API_KEY": "@api_key"
  }
}
